FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pointKineticsFoam;

startTime 0.0;

deltaT  0.000000000001;

endTime 100.0;

writeControl runTime;

writeInterval 10;

writeFormat ascii;

writePrecision 15;

